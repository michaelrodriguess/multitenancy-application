generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String
  email String
  passwd String
  roles Json
  createAt DateTime @default(now())
  updateAt DateTime @default(now())
  
  @@unique([email])
}